<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4000"
        motion:transitionDisable="true"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>

           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="0"
               android:scaleX="8" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="0"
               android:scaleY="8" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="85"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="75"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="99"
               android:alpha="1" />

           <KeyAttribute
               motion:motionTarget="@+id/animationView"
               motion:framePosition="75"
               android:alpha="10" />
           <KeyAttribute
               motion:motionTarget="@+id/animationView"
               motion:framePosition="85"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/animationView"
               motion:framePosition="99"
               android:alpha="0" />

           <KeyAttribute
               motion:motionTarget="@id/logo"
               motion:framePosition="0"
               android:alpha="0"/>
           <KeyAttribute
               motion:motionTarget="@id/logo"
               motion:framePosition="20"
               android:alpha="0"/>
        <KeyAttribute
               motion:motionTarget="@id/logo"
               motion:framePosition="40"
               android:alpha="0"/>
           <KeyAttribute
               motion:motionTarget="@id/logo"
               motion:framePosition="60"
               android:alpha="0"/>
            <KeyAttribute
               motion:motionTarget="@id/logo"
               motion:framePosition="65"
               android:alpha="-0.1"/>
           <KeyAttribute
               motion:motionTarget="@+id/logo"
               motion:framePosition="75"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@id/logo"
               motion:framePosition="90"
               android:alpha="1"/>
           <KeyAttribute
               motion:motionTarget="@+id/logo"
               motion:framePosition="99"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo"
               motion:framePosition="97"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo"
               motion:framePosition="88"
               android:alpha="0.4" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="0"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/animationView"
               motion:framePosition="0"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="1000"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/animationView"
               motion:framePosition="100"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="100"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="100"
               android:scaleX="8" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="100"
               android:scaleY="8" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="40"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/circle"
               motion:framePosition="40"
               android:scaleY="1.2" />
       </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="0.1"
            motion:maxVelocity="0.1" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>